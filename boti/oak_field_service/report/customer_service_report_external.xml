<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_report_customer_service_external" model="ir.actions.report">
        <field name="name">Customer Service Report External</field>
        <field
            name="model"
            model="ir.model"
            eval="obj().env.ref('oak_field_service.customer_service_worksheet_template').model_id.model"
        />
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >oak_field_service.report_customer_service_external</field>
        <field
            name="report_file"
        >oak_field_service.report_customer_service_external</field>
        <field name="print_report_name">'Customer Service Report (External)'</field>
        <field
            name="binding_model_id"
            model="ir.model"
            eval="obj().env.ref('oak_field_service.customer_service_worksheet_template').model_id.id"
        />
        <field name="binding_type">report</field>
    </record>

    <template id="report_customer_service_external">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t
                    t-call="oak_field_service.report_customer_service_external_document"
                />
            </t>
        </t>
    </template>

    <template id="report_customer_service_external_document">
        <t t-call="web.external_layout">
            <div class="page" style="font-size: 25px !important;" />
        </t>
    </template>

